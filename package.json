{
  "name": "atlassian-connect-firestore",
  "version": "1.0.0",
  "description": "Firestore Adapter for Atlassian Connect Express",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint -p .",
    "prepublish": "tsc"
  },
  "lint-staged": {
    "*.{ts,md}": [
      "yarn prettier --write",
      "git add"
    ],
    "src/**/*.ts": [
      "npm run lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antonversal/atlassian-connect-firestore"
  },
  "keywords": [
    "firestore",
    "atlassian",
    "firebase",
    "connect",
    "express"
  ],
  "author": "Anton Versal <ant.ver@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonversal/atlassian-connect-firestore/issues"
  },
  "homepage": "https://github.com/antonversal/atlassian-connect-firestore#readme",
  "devDependencies": {
    "@firebase/testing": "^0.7.3",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.5",
    "firebase-admin": "^11.11.1",
    "firebase-functions-test": "^0.1.6",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-extended": "^0.11.1",
    "jest-watch-typeahead": "^0.2.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {}
}
